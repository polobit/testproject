<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>com.agilecrm.HomeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    
    <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
	
	 <servlet>
        <servlet-name>Scribe</servlet-name>
        <servlet-class>com.agilecrm.ScribeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Scribe</servlet-name>
        <url-pattern>/scribe</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>GmailContextualGadget</servlet-name>
        <servlet-class>com.agilecrm.GMailGadgetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GmailContextualGadget</servlet-name>
        <url-pattern>/gmail</url-pattern>
    </servlet-mapping>
    
    <servlet>
		<servlet-name>Cron</servlet-name>
		<servlet-class>com.campaignio.cron.Cron</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cron</servlet-name>
		<url-pattern>/cron</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	    <servlet-name>login</servlet-name>
	     <servlet-class>com.agilecrm.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>login</servlet-name>
	    <url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>register</servlet-name>
	    <servlet-class>com.agilecrm.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>register</servlet-name>
	    <url-pattern>/register</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
   <servlet-name>Jersey REST Service</servlet-name>
  <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
     <init-param>
  			<param-name>com.sun.jersey.config.property.packages</param-name>
  			<param-value>com.agilecrm.core.api;org.codehaus.jackson.jaxrs</param-value>
		</init-param>
     <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Jersey REST Service</servlet-name>
    <url-pattern>/core/*</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>Jersey REST Service</servlet-name>
    <url-pattern>/dev/*</url-pattern>
  </servlet-mapping>
  
 
  <servlet>
    <servlet-name>OpenIdServlet</servlet-name>
    <servlet-class>com.agilecrm.session.OpenIdServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>OpenIdServlet</servlet-name>
    <url-pattern>/openid</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>CalendarServlet</servlet-name>
    <servlet-class>com.agilecrm.session.CalendarServlet</servlet-class>
    <init-param>
      <param-name>consumer_key</param-name>
      <param-value/>
    </init-param>
    <init-param>
      <param-name>consumer_secret</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>CalendarServlet</servlet-name>
    <url-pattern>/meeting</url-pattern>
  </servlet-mapping>
  
  <servlet>
 	<servlet-name>Reports</servlet-name>
 	<servlet-class>com.agilecrm.reports.ReportServlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
 	<servlet-name>Reports</servlet-name>
 	<url-pattern>/cron/reports/*</url-pattern>
 </servlet-mapping>

<servlet>
 	<servlet-name>Stripe Webhook</servlet-name>
 	<servlet-class>com.agilecrm.subscription.StripeWebhookServlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
 	<servlet-name>Stripe Webhook</servlet-name>
 	<url-pattern>/stripewebhook/*</url-pattern>
 </servlet-mapping>

<servlet>
  <servlet-name>ForgotDomain</servlet-name> 
  <jsp-file>/forgot-domain.jsp</jsp-file> 
  </servlet>
 <servlet-mapping>
 <servlet-name>ForgotDomain</servlet-name>
 <url-pattern>/forgot-domain</url-pattern> 
 </servlet-mapping>

<servlet>
 <servlet-name>ForgotPassword</servlet-name> 
 <jsp-file>/forgot-password.jsp</jsp-file> 
 </servlet>
 <servlet-mapping>
 <servlet-name>ForgotPassword</servlet-name> 
 <url-pattern>/forgot-password</url-pattern> 
 </servlet-mapping>
 
 <servlet>
 <servlet-name>ChooseDomain</servlet-name> 
<jsp-file>/choose-domain.jsp</jsp-file> 
</servlet>

<servlet-mapping>
<servlet-name>ChooseDomain</servlet-name> 
<url-pattern>/choose-domain</url-pattern> 
</servlet-mapping>
  
  <filter>
       <filter-name>NamespaceFilter</filter-name>
       <filter-class>com.agilecrm.filter.NamespaceFilter</filter-class>
   </filter>
   
   <filter-mapping>
       <filter-name>NamespaceFilter</filter-name>
       <url-pattern>/*</url-pattern>
   </filter-mapping>
   
    <filter>
       <filter-name>AuthFilter</filter-name>
       <filter-class>com.agilecrm.filter.AgileAuthFilter</filter-class>
   </filter>
  
  <filter-mapping>
       <filter-name>AuthFilter</filter-name>
           <url-pattern>/home</url-pattern>
           <url-pattern>/</url-pattern>
            <url-pattern>/core/*</url-pattern>
            <url-pattern>/scribe</url-pattern>
            <url-pattern>/upload.jsp</url-pattern>
   </filter-mapping>
   
    <filter>
       <filter-name>BasicAuthFilter</filter-name>
       <filter-class>com.agilecrm.filter.BasicAuthFilter</filter-class>
   </filter>
   
   <filter-mapping>
       <filter-name>BasicAuthFilter</filter-name>
       <url-pattern>/dev/*</url-pattern>
   </filter-mapping>
  
   
 <filter>
 	<filter-name>JSAPIFilter</filter-name>
 	<filter-class>com.agilecrm.filter.JSAPIFilter</filter-class>
 </filter>
 
 <filter-mapping>
 	<filter-name>JSAPIFilter</filter-name>
 	<url-pattern>/core/js/*</url-pattern>
 </filter-mapping>
   
   
   <welcome-file-list>
   <welcome-file>home</welcome-file>
   </welcome-file-list>
   
      
    <security-constraint>
        <web-resource-collection>
            <url-pattern>/*</url-pattern>
        </web-resource-collection>
        <user-data-constraint>
            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
        </user-data-constraint>
	</security-constraint>


 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>